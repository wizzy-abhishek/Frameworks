			com.password_saver.Cred_Store
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>
			first
		</servlet-name>
		<url-pattern>
			/feed
		</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>
			second
		</servlet-name>
		<servlet-class>
			com.password_saver.Cred_Out
